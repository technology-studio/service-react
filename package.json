{
  "name": "@txo/service-react",
  "version": "5.1.22",
  "description": "Technology Studio - Service react",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/technology-studio/service-react.git"
  },
  "author": {
    "name": "Technology Studio",
    "email": "npm@technologystudio.sk",
    "url": "https://github.com/technology-studio/"
  },
  "license": "UNLICENSED",
  "private": false,
  "scripts": {
    "build": "yarn build:clean && yarn build:lib",
    "build:clean": "yarn rimraf lib",
    "build:lib": "yarn tsc",
    "build:watch": "yarn tsc --watch",
    "test": "jest",
    "test:watch": "concurrently \"yarn build:watch\" \"jest --watch\"",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "compare-boilerplate-version": "./scripts/compare-boilerplate-version.sh",
    "lint": "eslint --max-warnings 0 .",
    "fixcode": "eslint . --fix",
    "git-hook": "yarn -s lint-staged",
    "prepare": "husky && yarn build",
    "print-txo-packages": "yarn list 2> /dev/null|grep @txo|sed 's/.*\\(@txo[^@]*\\)@^*\\([^ ]*\\).*/\\1@\\2/g'|sort|uniq",
    "sanity": "yarn lint:ci && yarn build && tsc --noEmit && yarn test --coverage && yarn compare-boilerplate-version && echo 'success'",
    "semantic-release": "semantic-release",
    "update-boilerplate-version": "./scripts/update-boilerplate-version.sh",
    "lint:ci": "yarn lint",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@txo/context-redux": "^3.0.38",
    "@txo/functional": "^4.6.19",
    "@txo/log": "^2.0.16",
    "@txo/redux": "^4.0.32",
    "@txo/service-prop": "^2.2.18",
    "lodash.get": "^4.4.2"
  },
  "peerDependencies": {
    "immutability-helper": "*",
    "react-redux": "^9.1.2",
    "redux-saga": "^1.3.0"
  },
  "devDependencies": {
    "@txo-peer-dep/log": "^4.0.4",
    "@txo/commitlint": "^1.0.19",
    "@txo/log-console": "^3.0.0",
    "@txo/semantic-release": "^2.0.8",
    "@txo/service-prop": "^2.2.18",
    "@txo/tsconfig": "^1.1.1",
    "@types/jest": "^29.5.13",
    "@types/lodash.get": "^4.4.9",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "concurrently": "^9.0.1",
    "eslint-config-txo-typescript-react": "^2.0.76",
    "husky": "^9.1.6",
    "immutability-helper": "^3.1.1",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "redux-saga": "^1.3.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.2"
  },
  "resolutions": {
    "@types/react": "^18.3.11"
  }
}
