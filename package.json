{
  "name": "@txo/service-react",
  "version": "5.1.7",
  "description": "Technology Studio - Service react",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/technology-studio/service-react.git"
  },
  "author": {
    "name": "Technology Studio",
    "email": "npm@technologystudio.sk",
    "url": "https://github.com/technology-studio/"
  },
  "license": "UNLICENSED",
  "private": false,
  "scripts": {
    "build": "yarn build:clean && yarn build:lib",
    "build:clean": "yarn rimraf lib",
    "build:lib": "yarn tsc",
    "build:watch": "yarn tsc --watch",
    "test": "jest",
    "test:watch": "concurrently \"yarn build:watch\" \"jest --watch\"",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "compare-boilerplate-version": "./scripts/compare-boilerplate-version.sh",
    "lint": "eslint --max-warnings 0 .",
    "fixcode": "eslint . --fix",
    "git-hook": "yarn -s lint-staged",
    "prepare": "husky install && yarn build",
    "print-txo-packages": "yarn list 2> /dev/null|grep @txo|sed 's/.*\\(@txo[^@]*\\)@^*\\([^ ]*\\).*/\\1@\\2/g'|sort|uniq",
    "sanity": "yarn lint && yarn build && tsc --noEmit && yarn test --coverage && yarn compare-boilerplate-version && echo 'success'",
    "semantic-release": "semantic-release",
    "update-boilerplate-version": "./scripts/update-boilerplate-version.sh"
  },
  "dependencies": {
    "@txo/context-redux": "^3.0.33",
    "@txo/functional": "^4.6.17",
    "@txo/log": "^2.0.14",
    "@txo/redux": "^4.0.29",
    "@txo/service-prop": "^2.2.16",
    "lodash.get": "^4.4.2"
  },
  "peerDependencies": {
    "immutability-helper": "*",
    "react-redux": "^9.0.1",
    "redux-saga": "^1.2.3"
  },
  "devDependencies": {
    "@txo-peer-dep/log": "^4.0.3",
    "@txo/commitlint": "^1.0.8",
    "@txo/log-console": "^3.0.0",
    "@txo/semantic-release": "^1.0.3",
    "@txo/service-prop": "^2.2.16",
    "@txo/tsconfig": "^1.1.1",
    "@types/jest": "^29.5.10",
    "@types/lodash.get": "^4.4.9",
    "@types/react": "^18.2.41",
    "@types/react-dom": "^18.2.17",
    "concurrently": "^8.2.2",
    "eslint-config-txo-typescript-react": "^2.0.11",
    "husky": "^8.0.3",
    "immutability-helper": "^3.1.1",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.0.1",
    "redux-saga": "^1.2.3",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.2"
  },
  "resolutions": {
    "@types/react": "^18.2.41"
  }
}
