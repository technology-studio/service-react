{
  "name": "@txo/service-react",
  "version": "5.0.14",
  "description": "Technology Studio - Service react",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/technology-studio/service-react.git"
  },
  "author": {
    "name": "Technology Studio",
    "email": "npm@technologystudio.sk",
    "url": "https://github.com/technology-studio/"
  },
  "license": "UNLICENSED",
  "private": false,
  "scripts": {
    "build": "yarn build:clean && yarn build:lib",
    "build:clean": "yarn rimraf lib",
    "build:lib": "yarn tsc",
    "build:watch": "yarn tsc --watch",
    "test": "jest",
    "test:watch": "concurrently \"yarn build:watch\" \"jest --watch\"",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "compare-boilerplate-version": "./scripts/compare-boilerplate-version.sh",
    "lint": "eslint --format=node_modules/eslint-formatter-pretty --ext .jsx,.js,.ts,.tsx .",
    "fixcode": "eslint --format=node_modules/eslint-formatter-pretty --ext .jsx,.js,.ts,.tsx . --fix",
    "git-hook": "yarn -s lint-staged",
    "prepare": "husky install && yarn build",
    "print-txo-packages": "yarn list 2> /dev/null|grep @txo|sed 's/.*\\(@txo[^@]*\\)@^*\\([^ ]*\\).*/\\1@\\2/g'|sort|uniq",
    "sanity": "yarn lint && yarn build && tsc --noEmit && yarn test && yarn compare-boilerplate-version && echo 'success'",
    "semantic-release": "semantic-release",
    "update-boilerplate-version": "./scripts/update-boilerplate-version.sh"
  },
  "dependencies": {
    "@txo/context-redux": "^3.0.8",
    "@txo/functional": "^4.6.0",
    "@txo/log": "^2.0.1",
    "@txo/redux": "^4.0.12",
    "@txo/service-prop": "^2.2.2",
    "lodash.get": "^4.4.2"
  },
  "peerDependencies": {
    "immutability-helper": "*",
    "react-redux": "^8.0.5",
    "redux-saga": "^1.2.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.4",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@txo-peer-dep/log": "^4.0.0",
    "@txo/log-console": "^3.0.0",
    "@txo/service-prop": "^2.2.2",
    "@types/jest": "^29.5.0",
    "@types/lodash.get": "^4.4.7",
    "@types/react": "^18.0.31",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "commitizen": "^4.3.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.37.0",
    "eslint-config-txo-typescript-react": "^1.0.67",
    "eslint-formatter-pretty": "^5.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-redux-saga": "^1.3.2",
    "husky": "^8.0.3",
    "immutability-helper": "^3.1.1",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "redux-saga": "^1.2.3",
    "rimraf": "^4.4.1",
    "semantic-release": "^20.1.3",
    "semantic-release-slack-bot": "^4.0.0",
    "ts-jest": "^29.0.5",
    "typescript": "^5.0.2"
  },
  "resolutions": {
    "@types/react": "^18.0.31"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
